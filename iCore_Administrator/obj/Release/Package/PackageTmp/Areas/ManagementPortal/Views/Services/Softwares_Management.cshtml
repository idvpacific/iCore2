                        </div>
                        <fieldset class="form-group position-relative has-icon-left input-divider-left" style="width:100%">
                            <input type="text" class="form-control" id="Modal_Device_Editor_Description" placeholder="" style="width:100%" maxlength="200">
                            <div class="form-control-position">
                                <i class="fa fa-sticky-note"></i>
                            </div>
                        </fieldset>
                    </div>
                    <div class="col-lg-12" id="Modal_Device_Wait" style="display:none;text-align:center;padding:10px">
                        <div class="lds-ripple"><div></div><div></div></div>
                    </div>
                    <input type="hidden" id="Modal_DeviceIDU" value="" />
                </div>
                <div class="modal-footer" id="Modal_Device_Btn" style="opacity:1;pointer-events:auto">
                    <button type="button" class="btn btn-primary" onclick="Device_Edit_Save()">Save changes</button>
                    <button type="button" class="btn btn-danger" data-dismiss="modal">Cancel</button>
                </div>
            </div>
        </div>
    </div>
}
