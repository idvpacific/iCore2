                                                        <div class="badge badge-pill @GroupTypeBadge" style="width:95px">@RW[3].ToString().Trim()</div>
                                                    </td>
                                                    @if (RW[6].ToString().Trim() == "0")
                                                    {
                                                        <td onclick="GroupType_GroupShow('@RW[1].ToString().Trim()')" style="text-align:center;cursor:pointer">
                                                            <div class="badge badge-pill badge-light-danger" style="width:70px">Disabled</div>
                                                        </td>
                                                    }
