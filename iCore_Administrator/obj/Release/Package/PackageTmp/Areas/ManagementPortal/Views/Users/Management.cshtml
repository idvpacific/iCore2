                                            <th style="width:75px;padding-left:30px">Status</th>
                                            <th style="width:20px"></th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @if (ViewBag.DT_GroupType != null)
                                        {
                                            string GroupTypeBadge = "";
                                            foreach (System.Data.DataRow RW in ViewBag.DT_GroupType)
                                            {
                                                GroupTypeBadge = "";
                                                switch (RW[2].ToString().Trim())
                                                {
                                                    case "1": { GroupTypeBadge = "badge-light-primary"; break; }
                                                    case "2": { GroupTypeBadge = "badge-light-danger"; break; }
                                                    case "3": { GroupTypeBadge = "badge-light-info"; break; }
                                                }
                                                <tr>
                                                    <td onclick="GroupType_GroupShow('@RW[1].ToString().Trim()')" style="text-align:center;cursor:pointer"><i class="fa fa-tag text-primary" style="font-size:25px"></i></td>
                                                    <td onclick="GroupType_GroupShow('@RW[1].ToString().Trim()')" style="cursor:pointer">@RW[4].ToString().Trim()</td>
                                                    <td onclick="GroupType_GroupShow('@RW[1].ToString().Trim()')" style="cursor:pointer">@RW[5].ToString().Trim()</td>
