    <script src="~/Theme/Base/app-assets/vendors/js/extensions/sweetalert2.all.min.js"></script>
    <script src="~/Theme/Base/app-assets/vendors/js/forms/select/select2.full.min.js"></script>
    <script>
        $(".select2").select2({ dropdownAutoWidth: true, width: '100%' });
        try {
            $('#DeviceMasterTable').DataTable();
        } catch (e) { }
        try {
            $('#DeviceGroupManagementTable').DataTable();
        } catch (e) { }
        try {
            $('#GroupDevicesTable').DataTable();
        } catch (e) { }
        function ReloadGridDevice() {
            try {
                document.getElementById("Device_Table").style.opacity = "0.7";
                document.getElementById("Device_Table").style.pointerEvents = "none";
                document.getElementById("Device_Table_Wait").style.display = "block";
                var formData = new FormData();
                formData.append("PID", '@ViewBag.Parent_ID');
                $.ajax({
                    url: "@Url.Action("Softwares_Management_Grid", "Services")", type: "POST", data: formData, dataType: 'json', contentType: false, processData: false, async: true,
                    error: function () {
                        document.getElementById("Device_Table_Wait").style.display = "none";
                        document.getElementById("Device_Table").style.opacity = "1";
                        document.getElementById("Device_Table").style.pointerEvents = "auto";
                        toastr.error('An error occurred while processing your request to reload softwares list', 'An error has occurred! [C02]', { "showMethod": "slideDown", "hideMethod": "slideUp", positionClass: 'toast-bottom-right', "progressBar": true, "closeButton": true, "timeOut": 8000 });
                    },
                    success: function (data)
                    {
                        var ErrorCode = ""; var ErrorResult = "";
                        $.each(data, function (i, item) { ErrorResult = item.Text; ErrorCode = item.Value; });
                        if (ErrorCode == "0")
                        {
                            toastr.success("Softwares information list successfully synced", 'Congratulations', { "showMethod": "slideDown", "hideMethod": "slideUp", positionClass: 'toast-bottom-right', "progressBar": true, "closeButton": true, "timeOut": 8000 });
                            document.getElementById("Device_Table_Wait").style.display = "none";
