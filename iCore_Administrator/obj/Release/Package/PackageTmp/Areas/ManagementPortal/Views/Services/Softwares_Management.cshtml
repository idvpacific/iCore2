                                            <th style="width:30px;"></th>
                                            <th style="padding-left:30px">Software Name</th>
                                            <th style="padding-left:30px">Description</th>
                                            <th style="width:75px;padding-left:30px">Unique ID</th>
                                            <th style="width:75px;padding-left:30px">Inventories</th>
                                            <th style="width:75px;padding-left:30px">Status</th>
                                            <th style="width:20px"></th>
                                        </tr>
                                    </tfoot>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <div class="modal fade text-left" id="Modal_Device_Editor" tabindex="-1" role="dialog" aria-hidden="true">
